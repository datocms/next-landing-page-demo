    "accept-language": "^3.0.18",
    "datocms-structured-text-utils": "^2.0.4",
    "eslint": "8.33.0",
    "eslint-config-next": "^14.2.2",
    "framer-motion": "^10.15.1",
    "gql": "^1.1.2",
    "graphql": "^16.7.1",
    "i18next": "^23.4.1",
    "i18next-resources-to-backend": "^1.1.4",
